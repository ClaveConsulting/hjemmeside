---
import IndexPage from '../components/clave-old/pages/index';
---

<IndexPage client:only="react" />
<style is:global>
    @font-face {
        font-family: 'basis-grotesque-medium-pro';
        src: url('/fonts/basis/basis-grotesque-medium-pro.eot');
        src: url('/fonts/basis/basis-grotesque-medium-pro.eot?#iefix') format('embedded-opentype'),
        url('/fonts/basis/basis-grotesque-medium-pro.woff2') format('woff2'),
        url('/fonts/basis/basis-grotesque-medium-pro.woff') format('woff'),
        url('/fonts/basis/basis-grotesque-medium-pro.ttf') format('truetype');
        font-weight: normal;
        font-style: normal;
    }

    @font-face {
        font-family: 'basis-grotesque-regular-pro';
        src: url('/fonts/basis/basis-grotesque-regular-pro.eot');
        src: url('/fonts/basis/basis-grotesque-regular-pro.eot?#iefix') format('embedded-opentype'),
        url('/fonts/basis/basis-grotesque-regular-pro.woff2') format('woff2'),
        url('/fonts/basis/basis-grotesque-regular-pro.woff') format('woff'),
        url('/fonts/basis/basis-grotesque-regular-pro.ttf') format('truetype');
